record(calcout, "ioc:auto:cnt") {
  field(SCAN, "1 second")
  field(CALC, "A+B")
  field(INPA, "ioc:auto:cnt.VAL NPP")
  field(INPB, "0.01")
  field(OCAL, "sin(A)")
  field(DOPT, "Use OCAL")
  field(OUT , "ioc:auto PP")
}

